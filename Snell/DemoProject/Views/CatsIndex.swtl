<html>
  <head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
  </head>
  <body>
    <div class="container">
      <h1>Your Cats:</h1>

      <% for cat in scope.locals["cats"] as! Array<Cat> { %>
        <li><%= cat.name %> (<%= cat.color %>)
      <% } %>

      <a href="/cats/new">Create a cat</a>
    </div>
  </body>
</html>